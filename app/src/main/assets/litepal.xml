<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!-- 数据库名 -->
    <dbname value="MedRec"></dbname>
    <!-- 数据库版本 -->

    <version value="1"></version>
    <!-- 映射模型 -->

    <list>

        <!--病历-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanMedRec"/>
        <!--病程-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanCourseOfDisease"/>
        <!--就诊人列表-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanVisitingPerson"/>
        <!--用户已关注医生列表-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanConcernList"/>
        <!--检查报告-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanPrescriptiom"/>
        <!--个人信息表-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanPersonalInformation"/>

        <!--健康咨询HealthyChat的表-->
        <mapping class="com.healthyfish.healthyfish.POJO.ImMsgBean" />
        <!--用户已购买的服务列表-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanServiceList" />
        <!--化验单-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanInspectionReport" />
        <!--挂号信息-->
        <mapping class="com.healthyfish.healthyfish.POJO.BeanMyAppointmentItem" />
    </list>

</litepal>


